<script>

		$(window).scroll(function() {
		
		$('#certified').each(function(){
		var imagePos = $(this).offset().top;
		
		var topOfWindow = $(window).scrollTop();
		var activeWindow = topOfWindow+400 ;
			if (imagePos < topOfWindow+700) 
			{
				$(this).removeClass("inVisible");
				$(this).addClass("fadeIn1");
			}
			else if(topOfWindow < activeWindow || topOfWindow > imagePos)
			{
				$(this).removeClass("fadeIn1");
				$(this).addClass("inVisible");
			}
		});
		
		$('#certifiedLogo').each(function(){
		var imagePos = $(this).offset().top;
		
		var topOfWindow = $(window).scrollTop();
		var activeWindow = topOfWindow+400 ;
			if (imagePos < topOfWindow+700) {
				$(this).addClass("slideUp");
			}
			else if(topOfWindow < activeWindow || topOfWindow > imagePos)
			{
				$(this).removeClass("slideUp");
				$(this).addClass("inVisible");
			}
		});
		
		$('#certifiedTxt').each(function(){
		var imagePos = $(this).offset().top;
		
		var topOfWindow = $(window).scrollTop();
		var activeWindow = topOfWindow+500 ;
			if (imagePos < topOfWindow+700) {
				$(this).addClass("slideDown");
			}
			else if(topOfWindow < activeWindow || topOfWindow > imagePos)
			{
				$(this).removeClass("slideDown");
				$(this).addClass("inVisible");
			}
		});
		
		$('#IndexWork').each(function(){
		var imagePos = $(this).offset().top;
		
