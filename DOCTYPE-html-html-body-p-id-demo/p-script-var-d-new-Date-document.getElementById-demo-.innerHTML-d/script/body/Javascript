<script type="text/javascript">
$(document).on('change','#responsiveMenu',function(){
	var url = $(this).val();
	$(this).attr('selected','selected');
	window.location.href = url;
	
});
</script>

<script>
function jobDescription(jobDescId){
$("#jobDescSlideleft").removeClass('slideLeft');
	var position = $("div#JobOpeing" ).offset();
	height = $("div#JobOpeing" ).height();
	width = $("div#JobOpeing" ).width();
	$("div#jobDescription").css({'height':height, position : "absolute", "z-index" :"101", "background-color" : "","opacity" : "1" } );
	//$("div#jobDescription").offset($("div#JobOpeing" ).offset());
	
	$.ajax({
		url:  "http://softidsolutions.com/wp-content/themes/ssis/career_ajax.php",
	    type: 'POST',
	    data: "jobDescId="+jobDescId, 
	    success: function(response) {
	    	$("#jobDescription").show(); 
		  	  $("#jobDescription").html(response); 
		  	  //var h_height = $("#TopArea1").css('min-height');
		  	 //window.location.href="#jobDescription";
		  	 //$("#jobDescription").css('padding', '10px' );
		  	$("#jobDescSlideleft").addClass('slideLeft');
			
		 }

		});
	
}
function postResume(jobId,jobtitle){
	$("#postResume").show();
	$("#positiontxt").val(jobtitle);
$('#test').show();
	// window.location.href="#postResume";
document.getElementById('test').scrollIntoView();

}
$(document).ready(function(){
	$("#email").live('change',function(){
		var email = $(this).val();
		$.ajax({
			url:  "http://softidsolutions.com/wp-content/themes/ssis/career_ajax.php",
		    type: 'POST',
		    data: "email="+email, 
		    success: function(response) {
			    var obj = JSON.parse(response);
			    if(obj.response == '1'){
					$("#message").html("<span style='color:red;'>You have attended for company last six months.</span>");
					return false;
				 }
			  	else{
			  		$("#message").html("");
			  		return true;
				 }
			 }

			});
		});
	
});

function close_desc()
{
$("#jobDescription").html(''); 
 $("#jobDescSlideleft").removeClass('slideLeft');
}
 </script>
