<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript" src="http://softidsolutions.com/wp-content/themes/ssis/js/jquery.realperson.js"></script>
<link rel="stylesheet" href="http://softidsolutions.com/wp-content/themes/ssis/css/jquery.realperson.css">
<script type="text/javascript" src="http://softidsolutions.com/wp-content/themes/ssis/js/jquery.validate.js"></script>
	  <script type="text/javascript">
	
	$(document).ready(function(){
		
		myvalidate();
		$('.error').validate({
		    errorClass: 'error'
		});
		
		$(function() {
			//alert("hihihi");
			$('#defaultReal').realperson();
		});
	});
	function myvalidate(){
		
		$.validator.addMethod("textOnly", function(value, element) {
			return /[0-9]*/.test(value);
		}, "Alpha Characters Only."
		);
		$("#resumeform").validate({
		
		rules:{
			phone:{
				required: true,
				maxlength: 15,
				minlength:10
			}	
			
	  	},
	  	errorPlacement: function(error, element) {
	        $(element).filter(':not(.valid)').addClass("invalid");
		$(element).filter(':not(.valid)').removeClass("success");
	    },
	      	success: function(error) {
	        $("#resumeform").find('.valid').removeClass("invalid").addClass("success");
	    }
	 });
	return false;
	}
	</script>



	

